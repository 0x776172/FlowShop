<Application x:Class="FlowShop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FlowShop"
             xmlns:vm="clr-namespace:FlowShop.ViewModels"
             xmlns:v="clr-namespace:FlowShop.Views"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <DataTemplate DataType="{x:Type vm:DataProduksiViewModel}">
                <v:DataProduksiView/>
            </DataTemplate>
            <DataTemplate DataType="{x:Type vm:GenAlgViewModel}">
                <v:AlgoritmaView/>
            </DataTemplate>
        </ResourceDictionary>
    </Application.Resources>
</Application>
